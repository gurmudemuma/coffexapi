{
  "name": "coffee-export-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run -c vitest.config.ts",
    "test:watch": "vitest watch -c vitest.config.ts",
    "test:coverage": "vitest run --coverage -c vitest.config.ts",
    "test:ui": "vitest --ui -c vitest.config.ts",
    "test:e2e": "playwright test",
    "test:performance": "vitest run src/__tests__/performance --reporter=verbose -c vitest.config.ts",
    "test:unit": "vitest run src/**/*.{test,spec}.{ts,tsx} --exclude=src/__tests__/performance/** --exclude=src/e2e/** -c vitest.config.ts",
    "test:all": "npm run test:unit && npm run test:performance && npm run test:e2e",
    "test:quality": "node ../run-test-quality.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "setup": "bash setup.sh",
    "setup:windows": "setup.bat",
    "diagnose": "bash diagnose-ipfs.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.19.11",
    "@mui/x-date-pickers": "^6.19.9",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.5",
    "aes-js": "^3.1.2",
    "axios": "^1.6.2",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "immer": "^10.0.3",
    "ipfs-http-client": "^60.0.1",
    "lucide-react": "^0.539.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.1.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/aes-js": "^3.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.9.2",
    "vite": "^5.0.0",
    "vitest": "^1.6.1"
  }
}
